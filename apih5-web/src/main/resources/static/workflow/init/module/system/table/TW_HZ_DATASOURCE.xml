<?xml version="1.0" encoding="UTF-8"?>
<!-- 多数据源可视化配置表结构定义 -->
<tableinfo>
	<tablename>
	    <!-- 表名 -->
	    <name>TW_HZ_DATASOURCE</name>
	    <!-- 主键名称 -->
	    <pkname>PK_TW_HZ_DATASOURCE</pkname>
		<!-- 主键，格式如(ID,NAME) -->
		<pk>(ID)</pk>   
		<!-- 字段列表 -->
		<fieldinfos>
			<!-- 字段属性 -->
			<fieldinfo fieldname="ID" typename="varchar"   length="32" isnull="0"></fieldinfo>
			<fieldinfo fieldname="DATABASETYPE" typename="varchar"   length="100"></fieldinfo>
			<fieldinfo fieldname="IDENTIFIER" typename="varchar"   length="100"></fieldinfo>
			<fieldinfo fieldname="CONNECTION_TYPE" typename="varchar"   length="100"></fieldinfo>
			<fieldinfo fieldname="DRIVER_CLASS" typename="varchar"   length="200"></fieldinfo>
			<fieldinfo fieldname="IS_DEFAULT" typename="varchar"   length="10"></fieldinfo>
			<fieldinfo fieldname="JDBC_URL" typename="varchar"   length="200"></fieldinfo>
			<fieldinfo fieldname="USER_NAME" typename="varchar"   length="100"></fieldinfo>
			<fieldinfo fieldname="PASSWORD" typename="varchar"   length="100"></fieldinfo>	
			<fieldinfo fieldname="DB_CONNECTION_TYPE" typename="varchar"   length="100"></fieldinfo>
			<fieldinfo fieldname="MAXPOOLSIZE" typename="varchar"   length="100"></fieldinfo>	
			<fieldinfo fieldname="MINPOOLSIZE" typename="varchar"   length="100"></fieldinfo>	
			<fieldinfo fieldname="INITIALPOOLSIZE" typename="varchar"   length="100"></fieldinfo>	
			<fieldinfo fieldname="MAXIDELTIME" typename="varchar"   length="100"></fieldinfo>
			<fieldinfo fieldname="LOB_BUFFERSIZE" typename="varchar"   length="100"></fieldinfo>
			<fieldinfo fieldname="LOB_PROCESSORCLASS" typename="varchar"   length="100"></fieldinfo>					
		</fieldinfos>		
	</tablename>
</tableinfo>