			<fieldinfo fieldname="SBEGINTIME" typename="varchar" length="50"></fieldinfo>
			<fieldinfo fieldname="SENDTIME" typename="varchar" length="50"></fieldinfo>
			<fieldinfo fieldname="XBEGINTIME" typename="varchar" length="50"></fieldinfo>
			<fieldinfo fieldname="XENDTIME" typename="varchar" length="50"></fieldinfo>
			<fieldinfo fieldname="UNITID" typename="lvarchar" length="500"></fieldinfo>
			<fieldinfo fieldname="F_DATE" typename="varchar" length="50"></fieldinfo>
		</fieldinfos>
	</tablename>
</tableinfo>