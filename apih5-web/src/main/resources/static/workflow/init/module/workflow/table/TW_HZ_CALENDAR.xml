			<fieldinfo fieldname="START_M" typename="varchar"   length="2"></fieldinfo>
			<fieldinfo fieldname="END_Y" typename="varchar"   length="4"></fieldinfo>
			<fieldinfo fieldname="END_M" typename="varchar"   length="2"></fieldinfo>
			<fieldinfo fieldname="ORGANID" typename="lvarchar"   length="500"></fieldinfo>
			<fieldinfo fieldname="HOUR_START" typename="varchar"   length="2"></fieldinfo>
			<fieldinfo fieldname="MIN_START" typename="varchar"   length="2"></fieldinfo>
			<fieldinfo fieldname="HOUR_END" typename="varchar"   length="2"></fieldinfo>
			<fieldinfo fieldname="MIN_END" typename="varchar"   length="2"></fieldinfo>
		</fieldinfos>
	</tablename>
	<tablename>
	    <!-- 表名 -->
	    <name>TW_HZ_ATTENDANCE_WORKDAY</name>
	    <!-- 主键名称 -->
	    <pkname>PK_TW_HZ_CALENDAR_DAY</pkname>
		<!-- 主键，格式如(ID,NAME) -->
		<pk>(ID)</pk>   
		<!-- 字段列表 -->
		<fieldinfos>
			<!-- 字段属性 -->
			<fieldinfo fieldname="ID" typename="varchar"   length="32" isnull="0"></fieldinfo>
			<fieldinfo fieldname="F_YEAR" typename="varchar"   length="50"></fieldinfo>
			<fieldinfo fieldname="F_MONTH" typename="varchar" length="50"></fieldinfo>
