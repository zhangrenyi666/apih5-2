<?xml version="1.0" encoding="UTF-8"?>
<!-- 表结构字段类型定义 -->
<fieldtypes>
	<types>
	    <!-- 字符串类型 -->
		<name>varchar</name>
		<db>
			<oracle>VARCHAR2</oracle>
			<db2>VARCHAR</db2>
			<mysql>VARCHAR</mysql>
			<dm>VARCHAR2</dm>
			<sqlserver>VARCHAR</sqlserver>
			<gbase>VARCHAR</gbase>
			<gbase8s>VARCHAR</gbase8s>
			<postgresql>VARCHAR</postgresql>
			<kingbase>VARCHAR</kingbase>
			<oscar>VARCHAR</oscar>
		</db>
	</types>
	<types>
	    <!-- 长字符串类型 -->
		<name>lvarchar</name>
		<db>
			<oracle>VARCHAR2</oracle>
			<db2>VARCHAR</db2>
			<mysql>VARCHAR</mysql>
			<dm>VARCHAR2</dm>
			<sqlserver>VARCHAR</sqlserver>
			<gbase>VARCHAR</gbase>
			<gbase8s>LVARCHAR</gbase8s>
			<postgresql>VARCHAR</postgresql>
			<kingbase>VARCHAR</kingbase>
			<oscar>VARCHAR</oscar>
		</db>
	</types>
	<!-- 数字类型 -->
	<types>
		<name>number</name>
		<db>
			<oracle>NUMBER</oracle>
			<db2>INT</db2>
			<mysql>INTEGER</mysql>
			<dm>NUMBER</dm>
			<sqlserver>INT</sqlserver>
			<gbase>INT</gbase>
			<gbase8s>INT</gbase8s>
			<postgresql>NUMERIC</postgresql>
			<kingbase>NUMBER</kingbase>
			<oscar>INT</oscar>
		</db>
	</types>
	<!-- clob大字段类型 -->
	<types>
		<name>clob</name>
		<db>
			<oracle>CLOB</oracle>
			<db2>CLOB</db2>
			<mysql>LONGTEXT</mysql>
			<dm>CLOB</dm>
			<sqlserver>VARCHAR(MAX)</sqlserver>
			<gbase>LONGTEXT</gbase>
			<gbase8s>CLOB</gbase8s>
			<postgresql>TEXT</postgresql>
			<kingbase>CLOB</kingbase>
			<oscar>CLOB</oscar>
		</db>
	</types>
	<!-- blob二进制类型 -->
	<types>
		<name>blob</name>
		<db>
			<oracle>BLOB</oracle>
			<db2>BLOB</db2>
			<mysql>LONGBLOB</mysql>
			<dm>BLOB</dm>
			<sqlserver>VARBINARY(MAX)</sqlserver>
			<gbase>BLOB</gbase>
			<gbase8s>BLOB</gbase8s>
			<postgresql>BYTEA</postgresql>
			<kingbase>BLOB</kingbase>
			<oscar>BLOB</oscar>
		</db>
	</types>
	<!-- Long类型 -->
	<types>
		<name>long</name>
		<db>
			<oracle>LONG</oracle>
			<db2>BIGINT</db2>
			<mysql>BIGINT</mysql>
			<dm>BIGINT</dm>
			<sqlserver>BIGINT</sqlserver>
			<gbase>BIGINT</gbase>
			<gbase8s>BIGINT</gbase8s>
			<postgresql>BIGINT</postgresql>
			<kingbase>BIGINT</kingbase>
			<oscar>BIGINT</oscar>
		</db>
	</types>
</fieldtypes>